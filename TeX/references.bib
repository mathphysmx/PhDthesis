	doi = {10.1112/S1461157015000236},
	abstract = {We present a parallel algorithm to calculate a numerical approximation of a single, isolated root \$\{{\textbackslash}it{\textbackslash}alpha\}\$ of a function \$f:{\textbackslash}mathbb\{R\}{\textbackslash}rightarrow {\textbackslash}mathbb\{R\}\$ which is sufficiently regular at and around \$\{{\textbackslash}it{\textbackslash}alpha\}\$. The algorithm is derivative free and performs one function evaluation on each processor per iteration. It requires at least three processors and can be scaled up to any number of these. The order with which the generated sequence of approximants converges to \$\{{\textbackslash}it{\textbackslash}alpha\}\$ is equal to \$(n+{\textbackslash}sqrt\{n{\textasciicircum}\{2\}+4\})/2\$ for \$n+1\$ processors with \$n{\textbackslash}geqslant 2\$. This assumes that particular combinations of the derivatives of \$f\$ do not vanish at \$\{{\textbackslash}it{\textbackslash}alpha\}\$.},
	number = {1},
	journal = {LMS Journal of Computation and Mathematics},
	author = {Nijmeijer, M. J. P.},
	year = {2015},
	pages = {713--729}
}

