	volume = {27},
	issn = {1436-3240, 1436-3259},
	shorttitle = {Non-parametric copulas for circular{\textendash}linear and circular{\textendash}circular data},
	url = {http://link.springer.com/article/10.1007/s00477-013-0733-y},
